<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
  <connectionStrings>
    <add name="StoreDB" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename='{0}StoreDB.mdf';Integrated Security=True;Connect Timeout=30" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="StoreService.StoreService">
        <endpoint address="http://localhost:8733/Design_Time_Addresses/StoreHost/StoreService/" behaviorConfiguration="StoreBehavior" binding="webHttpBinding" name="StoreEndpoint" contract="StoreService.IStoreService"/>
      </service>
    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="StoreBehavior">
          <webHttp helpEnabled="true"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
