<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <client>
            <endpoint address="net.msmq://localhost/private/BookShopQueue"
                binding="netMsmqBinding" bindingConfiguration="BookShopQueueEndpoint"
                contract="WarehouseServiceReference.IWarehouseService" name="BookShopQueueEndpoint">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WarehouseService/ws"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IWarehouseService"
                contract="WarehouseServiceReference.IWarehouseService" name="WSHttpBinding_IWarehouseService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
        <bindings>
            <netMsmqBinding>
                <binding name="BookShopQueueEndpoint" exactlyOnce="false">
                    <security mode="None" />
                </binding>
            </netMsmqBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IWarehouseService" />
            </wsHttpBinding>
        </bindings>
        
    </system.serviceModel>
</configuration>